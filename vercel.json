{
  "public": "public",
  "buildCommand": "echo 'Static deployment'",
  "outputDirectory": "public",
  "cleanUrls": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain; charset=utf-8"
        }
      ]
    }
  ]
}
